<?xml version="1.0"?>
<extension key="hu.elelmiszerbank.email-token" type="module">
  <file>email_token</file>
  <name>Custom email tokens</name>
  <description>Adds custom tokens</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Sandor Semsey</author>
    <email>semseysandor@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://github.com/semseysandor/hu.elelmiszerbank.email-token</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-12-12</releaseDate>
  <version>1.3</version>
  <develStage>stable</develStage>
  <compatibility>
