<?xml version="1.0"?>
<extension key="hu.es-progress.email-token" type="module">
    <file>email_token</file>
    <name>Custom email tokens</name>
    <description>Adds custom tokens</description>
    <license>AGPL-3.0</license>
    <maintainer>
        <author>Sandor Semsey</author>
        <email>sandor@es-progress.hu</email>
    </maintainer>
    <urls>
        <url desc="Main Extension Page">https://gitlab.com/semsey_sandor_civicrm/hu.es-progress.email-tokens</url>
        <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2021-05-11</releaseDate>
    <version>1.3.1</version>
    <develStage>stable</develStage>
    <compatibility>
        <ver>5.0</ver>
        <ver>5.18</ver>
        <ver>5.22</ver>
    </compatibility>
    <comments>Various custom tokens for emails</comments>
    <classloader>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <civix>
        <namespace>CRM/EmailToken</namespace>
    </civix>
</extension>
